{
  "version": 2,
  "builds": [
    { "src": "e-commerce-back/index.js", "use": "@vercel/node" },
    { "src": "e-commerce-front/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "e-commerce-back/index.js" },
    { "src": "/(.*)", "dest": "e-commerce-front/dist/$1" }
  ]
}
